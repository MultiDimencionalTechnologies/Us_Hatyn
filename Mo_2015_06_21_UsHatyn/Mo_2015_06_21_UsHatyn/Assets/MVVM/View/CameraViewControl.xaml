					<ScrollViewer Height="632.5" Width="819.667" Canvas.Left="93.333" Canvas.Top="157.5" Template="{DynamicResource ScrollViewerControlTemplate1}" IsManipulationEnabled="True" PanningMode="VerticalOnly">
						<ScrollViewer.OpacityMask>
							<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
								<GradientStop Color="#00000000" Offset="0"/>
								<GradientStop Offset="1"/>
